            %h4 March 2015
            .montheventbox
              .monthevent-box
                .monthevent-img
                  = image_tag "event-map.jpg"
                  .monthevent-info.clearfix
                    %h5
                      Coaching Workshop
                      %span 9AM Thusday 19th Feburary
                    = link_to 'View', "#", 'data-modal' => 'modal-1', class: 'monthevent-view btn md-trigger'
          .monthevent-people.clearfix
            %h4 April 2015
            .montheventbox
              .monthevent-box
                .monthevent-img
                  = image_tag "event-map.jpg"
                  .monthevent-info.clearfix
                    %h5
                      Drive Kick Off Event
                      %span 9AM Thusday 19th Feburary
                    = link_to 'View', "#", 'data-modal' => 'modal-1', class: 'monthevent-view btn md-trigger'
            .montheventbox
              .monthevent-box
                .monthevent-img
                  = image_tag "event-map.jpg"
