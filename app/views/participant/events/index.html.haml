- content_for(:title, 'Drive Events')
.main-panel
  .event-panel
    #eventTabs.clearfix.event-block
      = render partial: 'events_month_scroller'
      .eventtabs-detail{:style => "min-height: inherit;"}
        .eventtabs-title
          %h3 ARRIVA'S UPCOMING EVENTS FOR YOU
          %p Filter by months of the year, past and present. View an event to download material.
        #tabs-1.eventtabs-box.ui-tabs-panel.ui-widget-content.ui-corner-bottom{"aria-expanded" => "true", "aria-hidden" => "false", "aria-labelledby" => "ui-id-12", :role => "tabpanel"}
