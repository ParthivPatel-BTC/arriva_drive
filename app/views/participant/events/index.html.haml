.overlay{"data-sidebar-overlay" => ""}
#modal-1.md-modal.md-effect-5
  .md-content.eventpopup-panel
    .eventpopup-title
      %h3
        Coaching Workshop
        %span 9am - 4pm
      %p 53 Park Lane, London, W1K2FR
    .eventpup-map
      %img{:alt => "", :src => "images/event-map2.jpg"}/
      .info-schedule
        %h4 Information schedule
        %p This event is designed to further develop your fit plans through group coaching and exploring your 360 feedback and assessment outputs
        %h4 Download Material
        .material-box
          %h5 Map  Directions
          %p Download all the infomation for travel to and from the event
        .material-box
          %h5 Event Information
          %p Download all the infomation for the event
        .material-box
          %h5 Guest Speakers Information
          %p Download all the infomation for travel to and from the event
      .material-footer.clearfix
        %p
          Nedd help? Contact
          %a{:href => "mailto:Support@drivedb.net"} Support@drivedb.net
        %a.btn.addtocelendar{:href => "#"} Add to Calendar
      %button.md-close Close

:javascript
  $(document).ready(function(){
    $('#myStathalf2').circliful();
    $('.recentnote-slide').bxSlider({
      auto: true,
      controls:false
    });
    $( "#eventTabs" ).tabs();
    $(".mouseWheelButtons .carousel").jCarouselLite({
      btnNext: ".mouseWheelButtons .next",
      btnPrev: ".mouseWheelButtons .prev",
      mouseWheel: true,
      vertical: true,
      visible: 12,
      circular: false
    });
    model_popup();
  });
