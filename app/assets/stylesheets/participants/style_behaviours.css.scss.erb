*{ -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; vertical-align:top;}
.contentarea{ max-width:100%; width:100%; float:left;}
.welcomerow{ font-size:11px; color:#cacfd1; margin-bottom:20px;}
.welcomeboxrowleft{ width:50%; display:inline-block; float:left;}
.welcomerow h3{color:#c8d92b; font-size:16px; margin:0px 0px 5px 0px; padding:0px; text-transform:uppercase;}
.welcomeboxrow{ width:100%; float:left; margin-bottom:20px;}
.welcomeboxrow .leadership{ float:left; width:400px; text-transform:uppercase;}
.welcomeboxrow .leadership h2{color:#94a339; font-size:22px; margin:0px 0px 5px 0px; padding:0px; text-transform:uppercase;}
.welcomeboxrow .leadership span.leaderblock{ display:inline-block; margin-right:10px;}
.welcomeboxrow .leadership .checker{margin-right:5px;}
.progressbarblock{ width:50%; float:left; padding-right:3%;}
.progressbarblockrow{ margin-bottom:10px;}
.progressbarlabel{ width:100px; float:left; text-transform:uppercase; line-height:12px; font-size:10px;}
.progressbarchat{ margin-left:100px;}
.progress {overflow: hidden; height: 20px; margin-bottom: 20px; background-color: #bcbcbd; -webkit-box-shadow: inset 0 1px 2px rgba(0,0,0,.1);  box-shadow: inset 0 1px 2px rgba(0,0,0,.1); width: 50% !important; margin-left: 100px;}
.progress-bar {float: left; width: 0; height: 100%; font-size: 12px; line-height: 20px; color: #fff; text-align: center; background-color: #428bca; -webkit-box-shadow: inset 0 -1px 0 rgba(0,0,0,.15); box-shadow: inset 0 -1px 0 rgba(0,0,0,.15); -webkit-transition: width .6s ease; transition: width .6s ease;}
.progress-bar-success {background-color: #c7da2c}
.progress-bar-warning {background-color: #87a4ae}
.progress-bar-danger {background-color: #d9534f}
.sr-only {position: absolute; width: 1px; height: 1px; margin: -1px; padding: 0;  overflow: hidden; clip: rect(0,0,0,0); border: 0;}